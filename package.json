{
  "name": "node-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel app -s -d dist",
    "production": "npm run build && nodemon ./bin/production",
    "serve": "npm run build && cd ./dist && npx serve",
    "scrape": "node ./bin/scrape"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dashify": "^2.0.0",
    "debug": "~2.6.9",
    "express": "~4.17.1",
    "fs": "0.0.1-security",
    "http-errors": "~1.7.3",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-console": "^0.2.1",
    "babel-plugin-console-source": "^2.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
