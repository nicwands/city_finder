    "debug": "~2.6.9",
    "express": "~4.17.1",
    "fs": "0.0.1-security",
    "http-errors": "~1.7.3",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "pug": "^2.0.4",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
